{
    "name": "saleem189/cart",
    "description": "Laravel Shopping Cart Package Compatible with Laravel 5-12",
    "keywords": ["laravel", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        },
        {
            "name": "Saleem",
            "email": "youremail@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/translation": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Darryldecode\\": "src/Darryldecode"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Darryldecode\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Darryldecode\\Cart\\Facades\\CartFacade"
            }
        }
    }
}
